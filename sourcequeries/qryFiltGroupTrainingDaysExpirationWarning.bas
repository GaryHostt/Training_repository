SELECT DISTINCT qryFiltrationEmployees.[Employee #] AS [Received Training], qryFiltrationEmployees.[First Name], qryFiltrationEmployees.[Last Name], tblFiltrationQualifications.Area, qryFiltCustomTrainingGroup2.Qualification AS Qualification, qryFiltCustomTrainingGroup2.[Qualification Level] AS [Level], qryFiltCustomTrainingGroup2.[Date trained] AS [Date Trained], qryFiltCustomTrainingGroup2.[Version Trained] AS Version, qryFiltCustomTrainingGroup2.[Employee Number of Trainer] AS [Trainer #], qryFiltCustomTrainingGroup2.[Days since trained] AS [Days since trained], qryFiltCustomTrainingGroup.[Document Title] AS [Document Title]
FROM qryFiltCustomTrainingGroup, qryFiltrationEmployees, tblFiltrationQualifications, qryFiltCustomTrainingGroup2
WHERE (((qryFiltrationEmployees.[Employee #])=qryFiltCustomTrainingGroup2.[Trained Employee Number]) And ((qryFiltCustomTrainingGroup2.[Days since trained])>70) And ((tblFiltrationQualifications.ID)=qryFiltCustomTrainingGroup2.Qualification));

